@using TravelTripProje.Models.Siniflar
@model List<Hakkimizda>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımda</h3>
            @foreach (var x in Model)
            {
                <img src="@x.FotoUrl" alt="" style="width:100%;height:500px;" />

                <p><h4>@x.Aciklama</h4></p>
            }

        </div>
    </div>
</div>